<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/homeFragment">

    <fragment android:id="@+id/browserFragment"
              android:name="com.wbj.kotlin3.BrowserFragment"
              android:label="BrowserFragment"
              tools:layout="@layout/browser_fragment"/>

    <fragment android:id="@+id/EULAFragment"
              android:name="com.wbj.kotlin3.EULAFragment"
              android:label="EULAFragment"
              tools:layout="@layout/eula_fragment"/>

    <fragment android:id="@+id/feedbackFragment"
              android:name="com.wbj.kotlin3.FeedbackFragment"
              android:label="FeedbackFragment"
              tools:layout="@layout/feedback_fragment"/>

    <fragment android:id="@+id/helpFragment"
              android:name="com.wbj.kotlin3.HelpFragment"
              android:label="HelpFragment"
              tools:layout="@layout/help_fragment"/>

    <fragment android:id="@+id/statementFragment"
              android:name="com.wbj.kotlin3.StatementFragment"
              android:label="StatementFragment"
              tools:layout="@layout/statement_fragment"/>
    <fragment android:id="@+id/homeFragment" android:name="com.wbj.kotlin3.HomeFragment" android:label="fragment_home"
              tools:layout="@layout/fragment_home">
        <action android:id="@+id/action_homeFragment_to_browserFragment" app:destination="@id/browserFragment"/>
        <action android:id="@+id/action_homeFragment_to_feedbackFragment" app:destination="@id/feedbackFragment"/>
        <action android:id="@+id/action_homeFragment_to_statementFragment" app:destination="@id/statementFragment"/>
        <action android:id="@+id/action_homeFragment_to_helpFragment" app:destination="@id/helpFragment"/>
        <action android:id="@+id/action_homeFragment_to_EULAFragment" app:destination="@id/EULAFragment"/>
    </fragment>
</navigation>